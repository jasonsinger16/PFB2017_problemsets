#!/usr/bin/env python

#calculate the AT content of a DNA string

seq = ""
numA = seq.count('A')
numT = seq.count('T')
numC = seq.count('C')
numG = seq.count('G')
seqlen = len(seq)
ATcontent = (numA + numT)/seqlen
GCcontent = (numC + numG)/seqlen


print("number of A:", numA)
print("number of T:", numT)
print("number of C:", numC)
print("number of G:", numG)
print("total number of nt:", seqlen)
print("AT content of sequence:", ATcontent)
print("GC content of sequence:", GCcontent)
print("check: %AT + %GC =", ATcontent + GCcontent)
